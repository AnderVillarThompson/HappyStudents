import javax.swing.*;
import java.io.*;
public class Main {
    public static void main(String[] args) {
        int numberOfStudents= Integer.parseInt(JOptionPane.showInputDialog(null, "Number of students please"));
        int numberOfChoices= Integer.parseInt(JOptionPane.showInputDialog(null, "Number of choices please"));

        FileReader fr = new FileReader( "Trial.txt");
        fr.parse(numberOfStudents,numberOfChoices);
    }
}
