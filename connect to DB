<?php

// CREATE CONNECTION
$servername = "localhost";
$username = "root";
$password = "root";
$dbname = "ProjectAssigner";

$conn = new mysqli($servername, $username, $password, $dbname,8889,'/Applications/MAMP/tmp/mysql/mysql.sock');

// Check connection
if ($conn->connect_error) {
    die("Connection failed: " . $conn->connect_error);
}

